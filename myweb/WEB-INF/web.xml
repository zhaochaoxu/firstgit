  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CardHome</display-name>
    <servlet-name>CardHome</servlet-name>
    <servlet-class>com.it.myserver.CardHome</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CardHome</servlet-name>
    <url-pattern>/cardhome</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Cardel</display-name>
    <servlet-name>Cardel</servlet-name>
    <servlet-class>com.it.myserver.Cardel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Cardel</servlet-name>
    <url-pattern>/cardel</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Cardadd</display-name>
    <servlet-name>Cardadd</servlet-name>
    <servlet-class>com.it.myserver.Cardadd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Cardadd</servlet-name>
    <url-pattern>/cardadd</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>Myfilter</display-name>
    <filter-name>Myfilter</filter-name>
    <filter-class>com.it.myfilter.Myfilter</filter-class>
    <init-param>
      <param-name>uris</param-name>
      <param-value>/index,/login,/text</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>Myfilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>UTF</display-name>
    <filter-name>UTF</filter-name>
    <filter-class>com.it.myfilter.UTF</filter-class>
    <init-param>
      <param-name>encod</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>UTF</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>mylistener.applistener</listener-class>
  </listener>
  <context-param>
    <param-name>dburl</param-name>
    <param-value>jdbc:mysql://127.0.0.1:3306/library</param-value>
  </context-param>
  <context-param>
    <param-name>dbuser</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>dbpwd</param-name>
    <param-value>0314</param-value>
  </context-param>
  <listener>
    <listener-class>mylistener.Sessionlistener</listener-class>
  </listener>
  <servlet>
