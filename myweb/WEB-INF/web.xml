<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>myweb</display-name>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>20</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>Bookhome</display-name>
    <servlet-name>Bookhome</servlet-name>
    <servlet-class>com.it.myserver.Bookhome</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Bookhome</servlet-name>
    <url-pattern>/bookhome</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Bookdel</display-name>
    <servlet-name>Bookdel</servlet-name>
    <servlet-class>com.it.myserver.Bookdel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Bookdel</servlet-name>
    <url-pattern>/bookdel</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Bookinsert</display-name>
    <servlet-name>Bookinsert</servlet-name>
    <servlet-class>com.it.myserver.Bookinsert</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Bookinsert</servlet-name>
    <url-pattern>/bookinsert</url-pattern>
  </servlet-mapping>
  <servlet>
